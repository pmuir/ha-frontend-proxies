{
    "name": "Router",
    "version": "0.2.1",
    "slug": "router_proxy",
    "description": "Proxy for externally running Router",
    "url": "https://github.com/pmuir/ha-proxy-frontends/tree/master/router",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "init": true,
    "true": true,
    "ingress": true,
    "timeout": 30,
    "panel_icon": "mdi:router-network",
    "map": [
      "config:ro"
    ],
    "ports": {
      "8099/tcp": null
    },
    "ports_description": {
      "8099/tcp": "Frontend tcp-listen port"
    },
    "options": {
      "server": null
    },
    "schema": {
      "server": "url"
    },
    "image": "pmuir/zigbee2mqtt-proxy-{arch}"
}
